{
    "name": "aplus/debug",
    "type": "library",
    "description": "Aplus Framework Debug Library",
    "keywords": [
        "debug",
        "timer",
        "error",
        "handler",
        "error-handling",
        "exception-handling",
        "exception",
        "log",
        "error-reporting",
        "view"
    ],
    "homepage": "https://aplus-framework.com/docs/debug",
    "license": "MIT",
    "authors": [
        {
            "name": "Natan Felles",
            "email": "natanfelles@gmail.com",
            "homepage": "https://natanfelles.github.io"
        }
    ],
    "require": {
        "php": ">=8.0",
        "aplus/cli": "^1.12",
        "aplus/helpers": "^2.1",
        "aplus/language": "^1.4",
        "aplus/log": "^2.1"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "aplus/coding-standard": "^1.8",
        "ergebnis/composer-normalize": "^2.15",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpmd/phpmd": "^2.10",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Framework\\Debug\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Debug\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "email": "natanfelles@gmail.com",
        "issues": "https://gitlab.com/aplus-framework/libraries/debug/issues",
        "wiki": "https://gitlab.com/aplus-framework/libraries/debug/wikis/home",
        "source": "https://gitlab.com/aplus-framework/libraries/debug",
        "docs": "https://aplus-framework.gitlab.io/libraries/debug/docs/"
    }
}
